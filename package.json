{
  "name": "launch-buttons",
  "displayName": "Launch buttons",
  "description": "Add run/debug button to your vscode to execute commands in launch.json.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
		"workspaceContains:.vscode/launch.json"
	],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "launch-buttons.run",
        "title": "Launch: 00_Run",
        "category": "navigation",
        "icon": {
          "light": "images/build_black_24dp.svg",
          "dark": "images/build_white_24dp.svg"
        }
      },
      {
        "command": "launch-buttons.debug",
        "title": "Launch: 01_Debug",
        "category": "navigation",
        "icon": {
          "light": "images/build_black_24dp.svg",
          "dark": "images/build_white_24dp.svg"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "launch-buttons.run",
          "group": "navigation"
        },
        {
          "command": "launch-buttons.debug",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
